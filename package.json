{
  "name": "hqo_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "postcy:run": "npx nyc report --reporter json --reporter lcov --reporter text --reporter html",
    "cy:run:ci": "cypress run",
    "postcy:run:ci": "npx nyc report --reporter json",
    "test:unit": "jest --coverage",
    "test:unit:ci": "jest",
    "posttest:unit:ci": "npm run report:coverage",
    "test:e2e": "run-p --race serve cy:run",
    "test:e2e:ci": "run-p --race serve cy:run:ci",
    "test": "npm run test:unit && npm run test:e2e:ci",
    "posttest": "npm run coverage:merge",
    "coverage:merge": "node ./scripts/merge_coverage.js",
    "postcoverage:merge": "npm run report:coverage",
    "report:coverage": "nyc report --reporter lcov --reporter html --reporter text --reporter json --report-dir coverage -t reports",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@types/crypto-js": "^3.1.43",
    "@types/js-cookie": "^2.2.5",
    "@types/lz-string": "^1.3.33",
    "@types/vuelidate": "^0.7.11",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.17.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-typescript-vue": "^1.0.3",
    "core-js": "^3.6.4",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "flush-promises": "^1.0.2",
    "istanbul-reports": "^3.0.2",
    "jquery": "^3.4.1",
    "js-cookie": "^2.2.1",
    "lz-string": "^1.4.4",
    "material-icons": "^0.3.1",
    "register-service-worker": "^1.6.2",
    "secure-ls": "^1.2.6",
    "vue": "^2.6.11",
    "vue-audio": "0.0.12",
    "vue-router": "^3.1.5",
    "vuelidate": "^0.7.5",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.2.1",
    "@cypress/webpack-preprocessor": "^5.1.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^13.11.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-e2e-cypress": "^4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-plugin-unit-jest": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-typescript": "^5.0.1",
    "@vue/test-utils": "1.0.0-beta.31",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.5.0",
    "css-loader": "^3.5.2",
    "cypress": "^4.4.0",
    "cypress-vue-unit-test": "^1.11.7",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "istanbul-lib-coverage": "^3.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.1",
    "sass-loader": "^8.0.2",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.8.2",
    "typescript": "~3.7.5",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11"
  }
}
